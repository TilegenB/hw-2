anner-background.jpg);\n    background-color: $header-background-color;\n    background-repeat: no-repeat;\n    background-size: cover;\n    position: relative;\n}\n.header-nav {\n    max-width: 1170px;\n    margin: 0 auto;\n    margin-bottom: 130px;\n&__list {\n    display: flex;\n    flex-flow: row wrap;\n    align-items: center;\n    border-bottom: 2px solid $border-color;\n}\n&__item {\n    display: flex;\n    flex-flow: row wrap;\n    align-items: center;\n    min-height: 125px;\n}\n&__item:not(:last-child) {\n    margin-right: 15px;\n}\n&__link {\n    font-size: 13px;\n    text-transform: uppercase;\n    padding: 10px 20px;\n    color: $primary-color;\n}\n}\n\n.header-nav__link:hover,\n.header-nav__link:hover {\n    pointer-events: auto;\n    transition: border ease-in-out .2s;\n    border-bottom: 3px solid $border-color;\n}\n.headaer-nav__item--active {\n    position: relative;\n}\n.header-nav__link--active {\n    cursor: default;\n    color: $active-color;\n}\n.header-nav__link--active::after {\n    position: absolute;\n    width: 100%;\n    content: \"\";\n    bottom: 0px;\n    left: 0px;\n    border-bottom: 3px solid $active-color;\n}\n.banner {\n    width: 1170px;\n    margin: 0 auto;\n    color: $primary-color;\n    text-align: center;\n}\n.banner__title {\n    font-size: 64px;\n    margin-bottom: 30px;\n    font-weight: 600;\n    text-transform: uppercase;\n}\n.banner__text {\n    font-size: 24px;\n    font-weight: 400;\n    margin-bottom: 80px;\n}\n.banner__button {\n    padding: 20px 90px;\n    box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.250);\n    margin-bottom: 120px;\n}\n.main {\n    flex-shrink: 0;\n}\n\n.feature {\n    padding: 110px 170px;\n}\n.feature__list {\n    width: 1170px;   \n    display: flex;\n    flex-flow: row wrap;\n    align-items: center;\n}   \n.feature__item {\n    max-width: 370px;\n    display: flex;\n    flex-flow: column wrap;\n    align-items: center;\n    margin-bottom: 90px;\n}\n.feature__img {\n    margin-bottom: 30px;\n}\n.feature__title {\n    padding: 15px 20px;\n    margin-bottom: 20px;\n    text-transform: uppercase;\n    font-size: 24px;\n    font-weight: 800;\n}\n.feature__text {\n    color: $gray-color;\n    font-size: 18px;\n    text-align: center;\n}\n.footer {\n    background-color: azure;\n    min-height: 200px;\n}"},"lineCount":null}},"error":null,"hash":"0d7c7e22fb4e912e5fcda7c253ffac9f","cacheData":{"env":{}}}