on--red {\n    background-color: $button1-background-color;\n}\n\n.header {\n    min-height: 747px;\n    background-image: url(../img/banner-background.jpg);\n    background-color: $header-background-color;\n    background-repeat: no-repeat;\n    background-size: cover;\n    position: relative;\n}\n\n.header-nav {\n    max-width: 1170px;\n    margin: 0 auto;\n    margin-bottom: 130px;\n\n    &__list {\n        display: flex;\n        flex-flow: row wrap;\n        align-items: center;\n        border-bottom: 2px solid $border-color;\n    }\n\n    &__item {\n        display: flex;\n        flex-flow: row wrap;\n        align-items: center;\n        min-height: 125px;\n    }\n\n    &__item:not(:last-child) {\n        margin-right: 15px;\n    }\n\n    &__link {\n        font-size: 13px;\n        text-transform: uppercase;\n        padding: 10px 20px;\n        color: $primary-color;\n    }\n}\n\n.header-nav__link:hover,\n.header-nav__link:hover {\n    pointer-events: auto;\n    transition: border ease-in-out .2s;\n    border-bottom: 3px solid $border-color;\n}\n\n.headaer-nav__item--active {\n    position: relative;\n}\n\n.header-nav__link--active {\n    cursor: default;\n    color: $active-color;\n}\n\n.header-nav__link--active::after {\n    position: absolute;\n    width: 100%;\n    content: \"\";\n    bottom: 0px;\n    left: 0px;\n    border-bottom: 3px solid $active-color;\n}\n\n.banner {\n    width: 1170px;\n    margin: 0 auto;\n    color: $primary-color;\n    text-align: center;\n}\n\n.banner__title {\n    font-size: 64px;\n    margin-bottom: 30px;\n    font-weight: 600;\n    text-transform: uppercase;\n}\n\n.banner__text {\n    font-size: 24px;\n    font-weight: 400;\n    margin-bottom: 80px;\n}\n\n.banner__button {\n    padding: 20px 90px;\n    box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.250);\n    margin-bottom: 120px;\n}\n\n.banner__button:hover,\n.banner__button:focus {\n    background-color: $button1-hover-color;\n    transition: background-color ease-in .2s;\n}\n\n.banner__button--active {\n    box-shadow: 4px 4px 30px $button1-background-color;\n}\n\n.main {\n    flex-grow: 2;\n}\n\n.feature {\n    padding: 110px 170px;\n}\n\n.feature__list {\n    width: 1170px;\n    display: flex;\n    flex-flow: row wrap;\n    align-items: center;\n}\n\n.feature__item {\n    max-width: 370px;\n    display: flex;\n    flex-flow: column wrap;\n    align-items: center;\n    margin-bottom: 90px;\n}\n\n.feature__img {\n    margin-bottom: 30px;\n}\n\n.feature__title {\n    padding: 15px 20px;\n    margin-bottom: 20px;\n    text-transform: uppercase;\n    font-size: 24px;\n    font-weight: 800;\n}\n\n.feature__text {\n    color: $gray-color;\n    font-size: 18px;\n    text-align: center;\n}\n\n.our-work {\n    &__list {\n        display: flex;\n        flex-flow: row wrap;\n    }\n    &__item {\n        width: 25%;\n        position: relative;\n    }\n}\n.our-work-link__content {\n    display: none;\n}\n.our-work-link:hover .our-work-link__content,\n.our-work-link:focus .our-work-link__content {\n    display: block;\n    background-color: $our-work-background;\n    width: 377px;\n    height: 339px;\n    transition: all linear .2s;\n}\n.our-work-link__content {\n    text-align: center;\n    padding: 110px 0;\n    top: 1px;\n    left: 0px;\n    z-index: 10;\n    position: absolute;\n    content:\"\";\n    color: $primary-color;\n}\n.our-work-title {\n    text-transform: uppercase;\n    margin-bottom: 30px;\n    font-size: 24px;\n}\n.our-work-text {\n    margin-bottom: 15px;\n    font-size: 18px;\n}\n.footer {\n    flex-grow: 2;\n    background-color: azure;\n    min-height: 200px;\n}"},"lineCount":null}},"error":null,"hash":"b2309c7e9b908a3ff511255a47b34bcf","cacheData":{"env":{}}}